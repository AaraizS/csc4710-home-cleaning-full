{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "HOST": "@db_host",
    "DB_USER": "@db_user",
    "PASSWORD": "@db_password",
    "DATABASE": "@db_name",
    "DB_PORT": "@db_port",
    "PORT": "5052"
  }
}
